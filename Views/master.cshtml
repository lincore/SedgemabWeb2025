@using Umbraco.Cms.Core.Models.PublishedContent
@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    Layout = null;
}
<!doctype HTML>
<html lang="en">
<head>
 
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">   
    <title>@Model.Value("title", fallback: Fallback.ToAncestors)</title>
    <meta name="description" content="@Model.Value("metadescription", fallback: Fallback.ToAncestors)">
    <meta name="keywords" content="@Model.Value("keywords", fallback: Fallback.ToAncestors)">
   
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700&display=swap">
    <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/vendor/fontawesome/css/all.css">
    <link rel="stylesheet" href="/css/cssmain25.css">
    
    <script src="/vendor/jquery/jquery-3.7.1.slim.min.js"></script>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-N03795Q5ZM"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'G-N03795Q5ZM');
    </script>
    <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js" ></script>

    <script src="/vendor/bootstrap/togglemode.js" async></script>
@RenderSection("top", required:false)

</head>



<body>

    <header class="container-fluid container-lg pb-1">
        <!--100% wide until extra large breakpoint-->
      
        @Html.Partial("navbarpartial")

    </header>

    <main class="container-fluid container-lg pb-3" >
    @RenderBody()
</main>
    

    @Html.Partial("footerpartial")

</body>
</html>


